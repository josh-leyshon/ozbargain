{
  "name": "ozbargain",
  "version": "1.0.0",
  "main": "./src/app.tsx",
  "private": true,
  "scripts": {
    "start": "expo start --tunnel",
    "web": "expo start --web",
    "lint": "eslint --flag unstable_ts_config",
    "format": "dprint fmt",
    "test": "jest",
    "build": "expo export",
    "build:android:preview:local": "npx eas-cli build -p android --profile preview --local",
